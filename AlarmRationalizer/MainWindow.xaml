<Window x:Class="AlarmRationalizer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AlarmRationalizer"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="318" Width="517">
            <TabItem x:Name="tabImport" Header="Import">
                <Grid Background="#FFE5E5E5">
                    <TextBox x:Name="txtCSVFilePath" HorizontalAlignment="Left" Height="26" Margin="76,17,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="334"/>
                    <Button x:Name="btnBrowse" Content="Browse" HorizontalAlignment="Left" Height="26" Margin="415,17,0,0" VerticalAlignment="Top" Width="82" Click="btnBrowse_Click"/>
                    <Label x:Name="label" Content="CSV File " HorizontalAlignment="Left" Height="26" Margin="10,17,0,0" VerticalAlignment="Top" Width="103"/>
                </Grid>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
    </StackPanel>
</Window>
